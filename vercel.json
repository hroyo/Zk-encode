{
    "name": "SecureMeds",
    "builds": [
        {
            "src": "/src/main.jsx",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/.*",
            "dest": "/src/main.jsx"
        }
    ]
}